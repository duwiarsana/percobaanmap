      ]
    },
    // Bangli (5106)
    "5106": {
      name: "bangli",
      code: "5106",
      displayName: "Bangli",
      kecamatanFiles: [
        "id5106010_susut",
        "id5106020_bangli",
        "id5106030_tembuku",
        "id5106040_kintamani"
      ]
    },
    // Karangasem (5107)
    "5107": {
      name: "karangasem",
      code: "5107",
      displayName: "Karangasem",
      kecamatanFiles: [
        "id5107010_rendang",
        "id5107020_sidemen",
        "id5107030_manggis",
        "id5107040_karangasem",
